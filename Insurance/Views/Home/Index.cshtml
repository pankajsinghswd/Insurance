@{ 
    ViewBag.Title = "Index";
   Layout = "~/Views/Shared/_LayoutStatic_en.cshtml";
 }
<div class="stepForm homepage">
    <div class="container">
        <div class="row">
            <div class="col-lg-2">

                <div class="header_right">
                    <div class="header_tabs">
                        <div class="header_tabinner">
                            <a class="header_tab icon2">
                                <div class="header_tabtooltip">Vehicle insurance</div>
                            </a>
                            <a class="header_tab icon5">
                                <div class="header_tabtooltip">Medical</div>
                            </a>
                            <a class="header_tab icon6">
                                <div class="header_tabtooltip">Medical Malpractice</div>
                            </a>
                            <a class="header_tab icon4">
                                <div class="header_tabtooltip">Travel</div>
                            </a>

                            <a class="header_tab icon3">
                                <div class="header_tabtooltip">Life & Savings</div>
                            </a>
                            <a class="header_tab icon1" href="/Home/HouseHold">
                                <div class="header_tabtooltip">House Hold</div>
                            </a>
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-lg-10">
                <div class="bannerForm_mn">
                    <div class="bannerForm">
                        <div class="bannerForm__item bannerForm__heading">
                            <img src="~/Content/frontenten/img/quote.png"> Get a Free Insurance Quote
                        </div>
                        <div class="bannerForm__item bannerForm__box">


                            <div class="form_control form_control">
                                <p class="bannerForm__box--head">
                                    Purpose of Insurance
                                </p>
                            </div>



                            <div class="form_control form_control-50">
                                <div class="form_control-radio">
                                    <input type="radio" id="f-option-1" name="selector">

                                    <div class="check"></div>
                                    <label for="f-option-1">New Insurance</label>
                                </div>
                            </div>
                            <div class="form_control form_control-50">
                                <div class="form_control-radio">
                                    <input type="radio" id="f-option-2" name="selector">
                                    <div class="check"></div>
                                    <label for="f-option-2">Transfer of ownership</label>

                                </div>
                            </div>


                            <div class="form_control">
                                <select class="select">
                                    <option>Select Insurance</option>
                                    <option>Home Insurance</option>
                                    <option>Health Insurance</option>
                                    <option>Car Insurance</option>
                                </select>
                            </div>

                            <div class="form_control  form_control-50">
                                <input type="text" name="" class="inputtext" placeholder="First Name">
                            </div>
                            <div class="form_control  form_control-50">
                                <input type="text" name="" class="inputtext" placeholder="Last Name">
                            </div>
                            <div class="form_control">
                                <input type="text" name="" class="inputtext" placeholder="Email Address">
                            </div>
                            <div class="form_control form_control-50">
                                <input type="text" name="" class="inputtext" placeholder="Phone number">
                            </div>
                            <div class="form_control form_control-50">

                                <select class="select">
                                    <option>Your Age</option>
                                </select>

                            </div>

                            <div class="form_control">
                                <div class="form_control-checkbox">
                                    <input type="checkbox" id="cc">
                                    <label for="cc">I agree to the terms and conditions of the company</label>
                                </div>
                            </div>



                            <div class="form_control">
                                <div class="button_div">
                                    <button type="button" name="next" class="next button">Get Quotes</button>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




</div>
<div class="partner">
    <div class="container">
        <div class="row">
            <div class="partner_inn">

                <div class="partner_inn-head">Vehicle Insurance</div>
                <div class="owl-carousel owl-theme">

                    <div class="item"><img src="~/Content/frontenten/images/logo_2.png"></div>
                    <div class="item"><img src="~/Content/frontenten/images/logo_1.jpg"></div>
                    <div class="item"><img src="~/Content/frontenten/images/logo_3.png"></div>
                    <div class="item"><img src="~/Content/frontenten/images/logo_4.png"></div>
                    <div class="item"><img src="~/Content/frontenten/images/logo_5.png"></div>
                    <div class="item"><img src="~/Content/frontenten/images/logo_6.png"></div>

                </div>

            </div>
        </div>
    </div>
</div>