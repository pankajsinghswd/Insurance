@{ ViewBag.Title = "About Us";
    Layout = "~/Views/Shared/_LayoutStatic_en.cshtml"; }
<div class="stepForm">
    <div class="container">
        <div class="row">
            <div id="aboutUs"></div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
            getStaticContent();
        });
        function getStaticContent() {
            $.ajax({
                type: "POST",
                url: "/Account/GetStaticPageContent",
                data: '{pageId: 1}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $('#aboutUs').html(response);
                }
            });
        }
</script>
